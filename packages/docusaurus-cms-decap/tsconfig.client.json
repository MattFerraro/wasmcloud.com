{
  "extends": ["./tsconfig.base.json", "@docusaurus/tsconfig"],
  "compilerOptions": {
    "composite": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "tsBuildInfoFile": "${configDir}/lib/.tsbuildinfo-client",
    "noEmit": false,
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-jsx"
  },
  "include": ["src/client"]
}
